{% extends 'base.html.twig' %}

{% block title %}ServerVisit{% endblock %}

{% block body %}
    <h1>ServerVisit</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ server_visit.id }}</td>
            </tr>
            <tr>
                <th>VisitDate</th>
                <td>{{ server_visit.visitDate ? server_visit.visitDate|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Ip</th>
                <td>{{ server_visit.ip }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('server_visit_index') }}">back to list</a>

    {{ include('server_visit/_delete_form.html.twig') }}
{% endblock %}
